@model IEnumerable<MVC_Assignment.Models.Contact>
@{
    ViewBag.Title = "Index";
}
<style>
    tr,td,th{
        border:1px solid black;
    }
    
</style>
<h2>Index</h2>
<p>@Html.ActionLink("Create New", "Create")</p>

<table border="2px solid black" cellspacing="5" cellpadding="5">
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
     
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.Email</td>
            <td>
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }
</table>

