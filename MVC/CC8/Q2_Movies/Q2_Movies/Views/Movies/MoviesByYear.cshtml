@model IEnumerable<Q2_Movies.Models.Movies>
@{
    ViewBag.Title = "MoviesByYear";
}


<h2>Movies By Year</h2>

<table cellpadding="5" cellspacing="5">
    <tr>
        <th>Movie ID</th>
        <th>Movie</th>
        <th>Director</th>
        <th>Year</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.MovieId</td>
            <td>@item.MovieName</td>
            <td>@item.DirectorName</td>
            <td>@item.DateOfRelease.Year</td>
        </tr>
    }
</table>


